#def 定義函數
def ss(x):
  x=x**2
  return x  #回傳X
#上為函數
#下為函數帶入範例
x=3
x_op = ss(x)
print(x_op)
print(x)





def pp(x):mm    #因數分解
  print(x,"的因數有:")
  for i in range(1,x+1):
    if x % i == 0:
      print(i,end=" ")
num = int(input("請輸入正整數:"))
pp(num)




