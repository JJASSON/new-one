#a059: 完全平方和
#專題一
a = int(input())
b = int(input())
sum = 0
for i in range(a,b+1):
  for x in range(1,34):
    if i == x**2:
      sum = i + sum
print(sum)



#a740: 质因数之和
#專題二
a = int(input())
sum = 0
for i in range(1,1000):
  while a % i == 0:
    a = a / i
    sum += i
  if a == 1:
    break
print(sum)




